<extension xmlns="http://ns.adobe.com/air/extension/30.0">
  <id>com.myflashlab.air.extensions.ADBMobile</id>
  <versionNumber>4.17.0</versionNumber>
  <copyright>2018, MyFlashLabs Team. All rights reserved.</copyright>
	<description>
		<text xml:lang="en">Air Native Extension!</text> 
	</description> 
	<platforms>
  
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.myflashlab.ADBMobile.MyExtension</initializer>
				<finalizer>com.myflashlab.ADBMobile.MyExtension</finalizer>
			</applicationDeployment>
		</platform>
	
		<platform name="Android-x86">
			<applicationDeployment>
				<nativeLibrary>classes.jar</nativeLibrary>
				<initializer>com.myflashlab.ADBMobile.MyExtension</initializer>
				<finalizer>com.myflashlab.ADBMobile.MyExtension</finalizer>
			</applicationDeployment>
    </platform>
	
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>libexADBMobile.a</nativeLibrary>
				<initializer>comMyflashlabsADBMobileMyExtensionInitializer</initializer>
				<finalizer>comMyflashlabsADBMobileMyExtensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
	
		<platform name="default"> 
			<applicationDeployment/>
		</platform>
	
  </platforms>
</extension>

